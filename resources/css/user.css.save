
 * {
    box-sizing: border-box;
  }

  body {
    font-family: 'Gentium', sans-serif;
    overflow-x: hidden;

  }

  /* Logout confirmation logout popup */
  ul {
    list-style: none;
  }

  a {
    text-decoration: none;
  }

  .cd-popup {
    position: fixed;
    left: 0;
    top: 0;
    height: 100%;
    width: 100%;
    opacity: 0;
    visibility: hidden;
    -webkit-transition: opacity 0.3s 0s, visibility 0s 0.3s;
    -moz-transition: opacity 0.3s 0s, visibility 0s 0.3s;
    transition: opacity 0.3s 0s, visibility 0s 0.3s;
    font-family: 'Gentium', sans-serif;
  }

  .cd-popup.is-visible {
    opacity: 1;
    visibility: visible;
    -webkit-transition: opacity 0.3s 0s, visibility 0s 0s;
    -moz-transition: opacity 0.3s 0s, visibility 0s 0s;
    transition: opacity 0.3s 0s, visibility 0s 0s;
  }

  .cd-popup-container {
    position: relative;
    width: 90%;
    max-width: 350px;
    margin: 6em auto;
    background: #081426;
    border-radius: 1em 1em 1em 1em;
    text-align: center;
    -webkit-transform: translateY(-80px);
    -moz-transform: translateY(-80px);
    -ms-transform: translateY(-80px);
    -o-transform: translateY(-80px);
    transform: translateY(-80px);
    -webkit-transition-property: -webkit-transform;
    -moz-transition-property: -moz-transform;
    transition-property: transform;
    -webkit-transition-duration: 0.3s;
    -moz-transition-duration: 0.3s;
    transition-duration: 0.3s;
  }

  .cd-popup-container p {
    padding: 1.5em 1.5em;
    font-size: 18px;
    color: #ffffff;
  }

  .cd-popup-container .cd-buttons {
    margin: 10px;
    padding: 10px;
    display: inline-flex;
    justify-content: space-evenly;
  }

  .cd-popup-container .cd-buttons:after {
    content: "";
    display: block;
    clear: both;
  }

  .cd-popup-container .cd-buttons li {
    text-align: right;
    padding: 10px 10px;

  }

  .cd-popup-container .cd-buttons li a {
    text-decoration: none;
    background-color: antiquewhite;
    color: #0a0a0a;
    font-size: 20px;
    font-weight: 400;
    padding: 10px;
    border-radius: 20px;

  }

  .cd-popup-container .cd-buttons li:first-child a {
    margin-left: 140px;
    width: 100px;
    height: 50px;
    padding: 5px 25px;

  }

  .cd-popup-container .cd-buttons li:first-child a:hover {
    background-color: #081426;
    border: 2px solid antiquewhite;
    color: white;

  }

  .cd-popup-container .cd-buttons li:last-child a {
    width: 100px;
    height: 50px;
    padding: 5px 25px;


  }

  .cd-popup-container .cd-buttons li:last-child a:hover {
    background-color: #081426;
    border: 2px solid antiquewhite;
    color: white;

  }

  .cd-popup-container .cd-popup-close {
    position: absolute;
    top: -10px;
    left: -10px;
    width: 25px;
    height: 25px;
    background-color: #649253;
    border-radius: 50%;
    text-decoration: none;
  }

  .cd-popup-container .cd-popup-close:hover {

    background-color: #ffffff;
    color: #00214B;
  }

  .is-visible .cd-popup-container {
    -webkit-transform: translateY(0);
    -moz-transform: translateY(0);
    -ms-transform: translateY(0);
    -o-transform: translateY(0);
    transform: translateY(0);
  }

